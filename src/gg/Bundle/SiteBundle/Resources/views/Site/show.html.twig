{% extends "ggSiteBundle::layout.html.twig" %}
{% block title %}{{ parent() }} - Show{% endblock %}
{% block body %}
<div id="bloc">
<h1>Site</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Name</th>                <td>{{ entity.name }}</td>        </tr>
        <tr>
            <th>Date_create</th>                <td>{{ entity.datecreate|date('Y-m-d H:i:s') }}</td>        </tr>
        <tr>
            <th>Date_modif</th>                <td>{{ entity.datemodif|date('Y-m-d H:i:s') }}</td>        </tr>
        <tr>
            <th>Version</th>                <td>{{ entity.version }}</td>        </tr>
        <tr>
            <th>Url</th>                <td>{{ entity.url }}</td>        </tr>
        <tr>
            <th>Description</th>                <td>{{ entity.description }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('site') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('site_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('site_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
</div>
{% endblock %}